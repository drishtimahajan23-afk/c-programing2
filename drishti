//programme to use for loop
#include<stdio.h>
int main()
{
    int n;
    printf("enter the value of n:");
    scanf("%d",&n);
     int i,j;

      for (i=2;i<n;i=i+1)//outer loop
      {
          int flag=1;
          for (j=2;j<i;j=j+1)//inner loop
          {
              if (i%j==0)
              {
                  flag==0;
                  break;
              }

          }
          if (flag==1)
            printf("entered value is %d/n",i);

      }
}
