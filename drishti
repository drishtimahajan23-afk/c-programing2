//Programme to use if and else
#include<stdio.h>
int main()
{
 int a;
 printf("enter the value of a:") ;
 scanf("%d",&a);
 if (a==5)
    printf("%d is equal to 5",a);
else if(a==6)
    printf("entered value is 6");
    else if (a==7)
        printf("entered value is 7");
        else
           printf("entered value is other than 5,6 and 7");
}

